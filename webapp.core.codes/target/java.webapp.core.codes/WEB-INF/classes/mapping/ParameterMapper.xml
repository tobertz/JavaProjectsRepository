<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.java.webapp.core.codes.dao.ParameterDao">
	<select id="getParameterIntValueFromName" parameterType="java.lang.String" resultType="java.lang.Integer">
 		SELECT parameter_int_value FROM parameters where parameter_name=#{paramName}
	</select>
</mapper>