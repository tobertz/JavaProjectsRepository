<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.java.webapp.core.codes.dao.AccessDao">
	<resultMap id="access" type="Access">
		<id property="accessId" column="access_id" />
	 	<result property="accessName"		column="access_name" />
	    <result property="accessCode"		column="access_name" />
	    <result property="accessDescription"	column="access_description" />
	    <result property="accessTypeId"		column="access_type_id" />
	    <result property="accessBaseName"	column="access_base_name" />
	    <result property="accessUrl"		column="access_url" />
	    <result property="accessGroup"		column="access_group" />
	 	<result property="accessSequence"	column="access_sequence" />
	 	<result property="parentAccessId"	column="parent_access_id" />
		<result property="accessLevel"		column="access_level" />
		<result property="statusId"		column="status" />

		<result property="createdBy" column="created_by"/>
        <result property="createdDate" column="created_date"/>    
    	
    	<result property="modifiedBy" column="modified_by"/>
    	<result property="modifiedDate" column="modified_date"/>
	</resultMap>

</mapper>